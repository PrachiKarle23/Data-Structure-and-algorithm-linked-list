class Solution
{
    public:
    //Function to remove duplicates from unsorted linked list.
    Node * removeDuplicates( Node *head) 
    {
        Node* temp=head;
        while(temp!=NULL)
        {
            Node* prev=temp;
            Node* temp1=temp->next;
            while(temp1!=NULL)
            {
                if(temp->data==temp1->data)
                {
                    prev->next=temp1->next;
                    temp1=temp1->next;
                    continue;
                }
                prev=temp1;
                temp1=temp1->next;
            }
            temp=temp->next;
        }
        return head;
      
    }
};
